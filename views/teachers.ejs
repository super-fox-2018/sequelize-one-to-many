<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>All Teachers Data</title>
  </head>
  <body>
  <h1 align="center">All Teachers Data</h1>
  <h5 align="center"> <a href="/teachers/add">Add Teachers Data</a> </h5>
  <table border="1" align="center">
  <thead>
    <tr>
      <th> First Name </th>
      <th> Last Name </th>
      <th> Email </th>
      <th> Subject </th>
      <th> Options </th>
    </tr>
  </thead>
  <tbody>
    <% teachers.forEach(function(teacherData){ %>
      <tr>
        <td><%= teacherData.firstName %></td>
        <td><%= teacherData.lastName %></td>
        <td><%= teacherData.email %></td>
        <td><%= helper.addUnassigned(teacherData) %></td>
        <td> <a href="#">Edit</a> | <a href="#">Delete</a> </td>
      </tr>
    <% }) %>
  </tbody>
  </table>
  </body>

</html>
